@{
    ViewBag.Title = "Index";
    List<CURDDemo.Models.MyTableModel> myTables = Model;
}

<table class="table table-bordered table-striped">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Value</th>
        <th></th>
    </tr>
    @foreach (var item in myTables) {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Value</td>
            <td>
                <a href="@Url.Action("Edit", "CURD", new {@id=item.Id })">Edit</a>
                <a href="@Url.Action("Delete", "CURD", new {@id=item.Id })">Delete</a>
            </td>
        </tr>
    }
</table>

<a href="@Url.Action("Create","CURD")">Add Item</a>

